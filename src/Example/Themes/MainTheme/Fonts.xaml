<?xml version="1.0" encoding="utf-8"?>
<ResourceDictionary xmlns="http://xamarin.com/schemas/2014/forms"
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
    
    <OnPlatform x:Key="RegularFontFamily" x:TypeArguments="x:String">
        <On Platform="iOS">Raleway</On>
        <On Platform="Android">Fonts/Raleway-Regular.ttf#Raleway</On>
    </OnPlatform>
    
    <OnPlatform x:Key="SemiBoldFontFamily" x:TypeArguments="x:String">
        <On Platform="iOS">Raleway-SemiBold</On>
        <On Platform="Android">Fonts/Raleway-SemiBold.ttf#Raleway-SemiBold</On>
    </OnPlatform>
    
    <OnPlatform x:Key="BoldFontFamily" x:TypeArguments="x:String">
        <On Platform="iOS">Raleway-Bold</On>
        <On Platform="Android">Fonts/Raleway-Bold.ttf#Raleway-Bold</On>
    </OnPlatform>
    
</ResourceDictionary>